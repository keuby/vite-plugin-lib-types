{
  "name": "vite-plugin-lib-types",
  "version": "2.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "test": "vitest --run",
    "prepack": "pnpm test && pnpm build"
  },
  "engines": {
    "node": "^16.0.0",
    "pnpm": "^8.0.0"
  },
  "bin": {
    "gen-types": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./parser": {
      "require": "./dist/parser.cjs",
      "import": "./dist/parser.js",
      "types": "./dist/parser.d.ts"
    },
    "./transformer": {
      "require": "./dist/transformer.cjs",
      "import": "./dist/transformer.js",
      "types": "./dist/transformer.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "parser.d.ts",
    "transformer.d.ts",
    "api-extractor.json"
  ],
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "magic-string": "^0.30.2",
    "mri": "^1.2.0",
    "pathe": "^1.1.1",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "ts-morph": "^19.0.0",
    "tsconfig": "^7.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.22.10",
    "@types/babel__core": "^7.20.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitest/coverage-istanbul": "^0.34.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "estree-walker": "^3.0.3",
    "prettier": "^3.0.1",
    "tsup": "^7.2.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  },
  "peerDependencies": {
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
