{
  "name": "vite-plugin-lib-types",
  "version": "1.0.9",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "test": "vitest --run",
    "prepack": "pnpm test && pnpm build"
  },
  "engines": {
    "node": "^16.0.0",
    "pnpm": "^8.0.0"
  },
  "bugs": {
    "url": "https://github.com/keuby/vite-plugin-lib-types/issues"
  },
  "homepage": "https://github.com/keuby/vite-plugin-lib-types/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keuby/vite-plugin-lib-types.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./parser": {
      "require": "./dist/parser.cjs",
      "import": "./dist/parser.js",
      "types": "./dist/parser.d.ts"
    },
    "./transformer": {
      "require": "./dist/transformer.cjs",
      "import": "./dist/transformer.js",
      "types": "./dist/transformer.d.ts"
    },
    "./*": "./*"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "vite plugin",
    "typescript",
    "dts"
  ],
  "files": [
    "dist",
    "parser.d.ts",
    "transformer.d.ts",
    "README.md"
  ],
  "dependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-typescript": "^7.22.11",
    "@rollup/pluginutils": "^5.0.4",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "magic-string": "^0.30.3",
    "pathe": "^1.1.1",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "~5.1.1",
    "ts-morph": "^17.0.1",
    "tsconfig": "^7.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.22.11",
    "@types/babel__core": "^7.20.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/coverage-istanbul": "^0.34.3",
    "@vue/tsconfig": "~0.1.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "typescript": "~4.8.4",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "peerDependencies": {
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
