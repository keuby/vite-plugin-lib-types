{
  "name": "vite-plugin-lib-types",
  "version": "2.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "bin": {
    "gen-types": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./parser": {
      "require": "./dist/parser/index.js",
      "import": "./dist/parser/index.mjs",
      "types": "./dist/parser/index.d.ts"
    },
    "./transformer": {
      "require": "./dist/transformer/index.js",
      "import": "./dist/transformer/index.mjs",
      "types": "./dist/transformer/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "parser.d.ts",
    "transformer.d.ts",
    "api-extractor.json"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@microsoft/api-extractor": "^7.36.4",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "magic-string": "^0.30.2",
    "mri": "^1.2.0",
    "pathe": "^1.1.1",
    "ts-morph": "^19.0.0",
    "tsconfig": "^7.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.22.10",
    "@types/babel__core": "^7.20.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "estree-walker": "^3.0.3",
    "prettier": "^3.0.1",
    "rollup": "^3.28.0",
    "tsup": "^7.2.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
