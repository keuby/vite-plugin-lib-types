{
  "mainEntryPointFilePath": "./dist/index.d.ts",
  "dtsRollup": {
    "enabled": true
  },
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "newlineKind": "lf",
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "error"
      },
      "TS2307": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-missing-release-tag": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
